<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.CapstoneProject.IOrder">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.CapstoneProject.IOrder.activity.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.CapstoneProject.IOrder.activity.Details" />
        <activity android:name="com.example.CapstoneProject.IOrder.activity.CartActivity" />
        <activity android:name="com.example.CapstoneProject.IOrder.activity.OrderActivity" />
        <activity android:name="com.example.CapstoneProject.IOrder.activity.ConfirmationActivity" />
        <activity
            android:name="com.example.CapstoneProject.IOrder.activity.MainActivity"
            android:parentActivityName="com.example.CapstoneProject.IOrder.activity.Login" />
        <activity android:name="com.example.CapstoneProject.IOrder.AdminLogin" />
        <activity android:name="com.example.CapstoneProject.IOrder.AdminView"></activity>
    </application>

</manifest>